<connector>
  <connectorId>NuxeoOperationConnector</connectorId>
  <version>5.0</version>
  <categories>
    <category>
      <name>Nuxeo</name>
      <icon>org/bonitasoft/connectors/nuxeo/avatar_nuxeo.png</icon>
    </category>
  </categories>
  <inputs>
    <setter>
      <setterName>setUserName</setterName>
      <required></required>
      <parameters>
        <string></string>
      </parameters>
    </setter>
    <setter>
      <setterName>setUserPassword</setterName>
      <required></required>
      <parameters>
        <string></string>
      </parameters>
    </setter>
    <setter>
      <setterName>setUrl</setterName>
      <required></required>
      <parameters>
        <string></string>
      </parameters>
    </setter>
    <setter>
      <setterName>setOperationId</setterName>
      <required></required>
      <parameters>
        <string></string>
      </parameters>
    </setter>
    <setter>
      <setterName>setInputType</setterName>
      <required></required>
      <parameters>
        <string></string>
      </parameters>
    </setter>
    <setter>
      <setterName>setOperationInput</setterName>
      <parameters>
        <object/>
      </parameters>
    </setter>
    <setter>
      <setterName>setParams</setterName>
      <parameters>
        <object/>
      </parameters>
    </setter>
  </inputs>
  <outputs>
    <getter>
      <name>operationOutput</name>
      <metadata/>
    </getter>
  </outputs>
  <pages>
    <page>
      <pageId>login</pageId>
      <widgets>
        <text>
          <labelId>userName</labelId>
          <setter reference="/connector/inputs/setter"/>
          <size>20</size>
          <maxChar>40</maxChar>
        </text>
        <password>
          <labelId>userPassword</labelId>
          <setter reference="/connector/inputs/setter[2]"/>
          <size>20</size>
          <maxChar>40</maxChar>
        </password>
        <text>
          <labelId>url</labelId>
          <setter reference="/connector/inputs/setter[3]"/>
          <size>20</size>
          <maxChar>40</maxChar>
        </text>
      </widgets>
    </page>
    <page>
      <pageId>operationConfiguration</pageId>
      <widgets>
        <text>
          <labelId>operationId</labelId>
          <setter reference="/connector/inputs/setter[4]"/>
          <size>20</size>
          <maxChar>40</maxChar>
        </text>
        <text>
          <labelId>inputType</labelId>
          <setter reference="/connector/inputs/setter[5]"/>
          <size>20</size>
          <maxChar>40</maxChar>
        </text>
        <text>
          <labelId>operationInput</labelId>
          <setter reference="/connector/inputs/setter[6]"/>
          <size>20</size>
          <maxChar>40</maxChar>
        </text>
        <text>
          <labelId>params</labelId>
          <setter reference="/connector/inputs/setter[7]"/>
          <size>20</size>
          <maxChar>40</maxChar>
        </text>
      </widgets>
    </page>
  </pages>
</connector>